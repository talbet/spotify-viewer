{
  "env": {
      "browser": true
  },
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "airbnb-base",
  "plugins": [
    "html",
    "vue"
  ],
  "rules": {
    "arrow-parens": ["error", "as-needed", {
      "requireForBlockBody": false,
    }],
  }
}
